  "dependencies": {
    "@appear.sh/introspector": "workspace:*",
    "fast-json-stable-stringify": "^2.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@swc/core": "^1.11.13",
    "@types/node": "^22.13.11",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
