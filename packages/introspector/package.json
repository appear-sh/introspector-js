{
  "name": "@appear.sh/introspector",
  "version": "1.3.0",
  "description": "JavaScript introspection library",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "hook.mjs"
  ],
  "typesVersions": {
    "*": {
      "node": [
        "./dist/node.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "import": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./node": {
      "import": {
        "import": "./dist/node.mjs",
        "types": "./dist/node.d.mts"
      },
      "default": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      }
    },
    "./nextjs": {
      "import": {
        "import": "./dist/nextjs.mjs",
        "types": "./dist/nextjs.d.mts"
      },
      "default": {
        "types": "./dist/nextjs.d.ts",
        "default": "./dist/nextjs.js"
      }
    }
  },
  "scripts": {
    "test": "vitest",
    "test:types": "npx --yes @arethetypeswrong/cli --pack .",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "prettier .",
    "lint:fix": "prettier --write .",
    "prepublish": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appear-sh/introspector-js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appear-sh/introspector-js/issues"
  },
  "homepage": "https://github.com/appear-sh/introspector-js#readme",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.200.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/instrumentation": "^0.200.0",
    "@opentelemetry/instrumentation-http": "^0.200.0",
    "@opentelemetry/instrumentation-undici": "^0.11.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "fast-json-stable-stringify": "^2.1.0",
    "lodash": "^4.17.21",
    "xxhashjs": "^0.2.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@types/xxhashjs": "^0.2.4",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
